    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graf"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/nasaApiFragment"
        android:name="ru.dw.material.view.layout.constraint.NasaApiFragment"
        android:label="NasaApiFragment" />
    <fragment
        android:id="@+id/layoutFragment"
        android:name="ru.dw.material.view.layout.LayoutFragment"
        android:label="LayoutFragment" />
    <fragment
        android:id="@+id/animationFragment"
        android:name="ru.dw.material.view.animation.AnimationFragment"
        android:label="AnimationFragment" />
    <action android:id="@+id/action_global_animationFragment" app:destination="@id/animationFragment" />
    <action android:id="@+id/action_global_layoutFragment" app:destination="@id/layoutFragment" />
    <action android:id="@+id/action_global_nasaApiFragment2" app:destination="@id/nasaApiFragment" />
    <fragment
        android:id="@+id/splashScreenFragment"
